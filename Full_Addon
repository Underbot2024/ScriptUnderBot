local allowedNames = {
  "Mexcaly",
  "name2",
  "name3",
}

if not table.find(allowedNames, name(), true) then
    --Made By VivoDibra
  return modules.game_textwindow.onGameEditText(0, 6279, 10000, name() ..", Tu no tienes permitido usar estos scripts.", "Scripts Underbot", "")
end

modules.game_textmessage.displayGameMessage("Scripts Protegidos Cargados.")

macro(100, "Protected Dance", function()
  local delay = 0
  for i=0,3 do
      schedule(delay, function() 
          player:setDirection(i)
      end)
      delay = delay + 20
  end
end)

macro(100, "Full Addon", function()
    local myOutfit = player:getOutfit()
    if myOutfit.addons ~= 3 then
      myOutfit.addons = 3
      player:setOutfit(myOutfit)
    end
  end)
